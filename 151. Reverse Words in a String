import java.util.StringTokenizer;
class Solution {
    public String reverseWords(String s) {
        StringTokenizer st=new StringTokenizer(s) ;
        //StringTokenizer st = new StringTokenizer(s);
          
        String s1= st.nextToken();
        while(st.hasMoreTokens()){
               s1 = st.nextToken() + " " + s1;
        }
       return s1 ;
    }
}
